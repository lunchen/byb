/**joinerManage.wxss**/
.container.joinerManage{
  height: 100%;
  background: #f5f5f5;
}
.joinerManage-screen-view{
  height: 120rpx;
  background: #fff;
  display: flex;
  align-items: center;
}
.joinerManage-screen-view-item{
  width: 50%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.screen-date{
  width: 220rpx;
  height: 56rpx;
  border: 1rpx #feb707 solid;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
  border-radius: 6rpx;
}
.screen-status{
  
  width: 260rpx;
  height: 56rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 24rpx;
  border-radius: 6rpx;
}
.screen-date-text{
  line-height: 56rpx;
}
.screen-date image{
  width: 28rpx;
  height: 28rpx;
}


.search-title{
  margin-left: 30rpx;
  color: #000;
  font-size: 32rpx;
}
.search-theme{
  padding: 30rpx 30rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.search-theme-item{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 68rpx;
  line-height: 68rpx;
  background: #f5f5f5;
  border-radius: 34rpx;
  padding: 0 30rpx 0 20rpx;
  margin: 0 20rpx 30rpx 0;
  font-size: 28rpx;
  flex: 0 0 auto;
}
.activity-view{
  padding: 0 30rpx;
}

.joinerManage-total{
  padding: 30rpx;
  font-size: 24rpx;
  color: #db0000;
}
.joinerManage-orderList{
  padding: 0 30rpx;
}
.joinerManage-orderList-item{
  height: 220rpx;
  background: #fff;
  border-radius: 14rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}
.joinerManage-orderList-item-left{
  margin: 0 50rpx;
}
.joinerManage-orderList-item-left .selected{
  width:30rpx;
  height:30rpx;
}
.joinerManage-orderList-item-left .select{
  width:30rpx;
  height:30rpx;
  border-radius: 1rpx;
  border: 1rpx #feb707 solid;
}
.joinerManage-orderList-item-middle{
  flex: 1 1 auto;
}
.joinerManage-orderList-item-middle-title{
  color: #333333;
  margin-bottom: 10rpx;
  font-size:30rpx;
  display: flex;
  align-items: center;
  flex: 1 1 auto;
}
.joinerManage-orderList-item-middle-text1{
  color: #666666;
  margin-bottom: 6rpx;
}
.joinerManage-orderList-item-middle-text2{
  color: #999999;
  font-size:24rpx;
  margin-bottom: 6rpx;
}
.joinerManage-orderList-item-right{
  margin:0 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #feb707;
}
.joinerManage-orderList-item-right .status1{
  color: #feb707;
  margin-right: 0rpx;
}
.joinerManage-orderList-item-right .status1{
  color: #fe8800;
  margin-right: 20rpx;
}
.joinerManage-orderList-item-right .status1{
  color: #999999;
  margin-right: 20rpx;
}
.exportExcel{
  position: fixed;
  right: 30rpx;
  bottom: 80rpx;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 8rpx;
  border: 1rpx #fe8800 solid;
  color: #fe8800;
}