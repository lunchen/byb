.business-content {
  position: relative;
  background: #fff;
  padding: 40rpx 30rpx 200rpx;
}
.business-content-introduction-title{
  color: #fe8800;
  font-size: 36rpx;
  background: transparent
}

/* 小三角 */
.business-triangle{
  content: '';
  width: 0px;
  height: 0px;
  border: 20rpx solid transparent;
  border-bottom: 30rpx solid #f5f5f5;
  margin: 5rpx 0 0 60rpx;
}
.business-content-introduction-info{
  background: #f5f5f5;
  border-radius: 26rpx;
  padding: 40rpx 37rpx 30rpx 30rpx;
}
.business-content-introduction-info-title{
  font-size: 30rpx;
  color: #333333;
  font-weight: bolder;
}
.business-content-introduction-info-text{
  margin-top: 24rpx;
  font-size: 26rpx;
  line-height: 44rpx;
  color: #333333;
}
.business-line{
  height: 1px;
  background: #f0f0f0;
  margin: 40rpx 0;
}
.contactInfor{
}
.contactInfo-title{
  color: #fe8800;
  font-size: 36rpx;
  margin-bottom: 30rpx;
}
.contactInfo-text{
  color: #666666;
  font-size: 30rpx;
  margin-bottom: 16rpx;
}