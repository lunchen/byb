/* signUpSuccess.wxss */
.container.signUpSuccess{
  background: #f5f5f5;
}
.signUpSuccess-view{
  padding: 50rpx 30rpx 0;
}
.signUpSuccess-view-title{
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333333;
  display: flex;
  justify-content: center;
}
.signUpSuccess-view-title image{
  height: 80rpx;
  width: 80rpx;
  margin-right: 30rpx;
}
.signUpSuccess-view-middle{
  width: 570rpx;
  padding: 30rpx 30rpx 15rpx;
  margin: 50rpx auto 0;
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0px 1px 4px 1px #eee;
}
.signUpSuccess-view-middle-imgView{
  width: 570rpx;
  height: 350rpx;
  border-radius: 10rpx;
  background: #eee;
}
.signUpSuccess-view-middle-img{
  width: 570rpx;
  height: 350rpx;
  border-radius: 10rpx;
}
.signUpSuccess-view-middle-title{
  font-size: 30rpx;
  color: #333333;
  margin: 30rpx 0 18rpx;
}
.signUpSuccess-view-middle-text{
  font-size: 24rpx;
  color: #999999;
  margin-bottom: 15rpx;
}
.signUpSuccess-shareToFriend{
  width: 270rpx;
  min-height: 60rpx;
  line-height: 60rpx;
  height: 60rpx;
  position: relative;
  margin: 90rpx auto 0;
  padding: 0;
  background: transparent;
}
.signUpSuccess-shareToFriend image{
  width: 270rpx;
  height: 60rpx;
}
.signUpSuccess-shareToFriend view{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.signUpSuccess-shareView{
  padding: 50rpx 70rpx;
  display: flex;
  justify-content: center;
}
.signUpSuccess-shareView-item{
  text-align: center;
  color: #666666;
  font-size: 28rpx;
  margin: 0 40rpx;
}
signUpSuccess-shareView-item-text{
  text-align: center;
  color: #666666;
  font-size: 28rpx;
  margin-right: 80rpx;
  
}
.signUpSuccess-shareView-item image{
  width: 64rpx;
  height: 64rpx;
}
.signUpSuccess-shareView-item button:not([size='mini']) {
  width: 64rpx;
  min-height: 0;
  height: 64rpx;
  margin-bottom: 20rpx; 
}
lineh20{
  line-height: 1;
}

button.signUpSuccess-shareView-item-buttom{
  text-align: center;
  background: transparent;
  padding: 0;
  margin: 0;
  font-size: 28rpx;
  color: #666666;
  width: 64rpx;
  height: 64rpx;
  font-weight: 0;
}
.videoTop button:not([size='mini']) {
  width: 100rpx;
}
.signUpSuccess-shareList{
  text-align: center;
  margin-top: 20rpx;
}


.canvasImg{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 110;
}
.canvasImg-bg{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  transition: opacity linear 200ms;
}
.canvasImg-view{
  width: 100%;
  height: 145vw;
  border-radius: 10rpx;
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 40rpx 10rpx 0; 
}
.canvasImg-view-top{
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-direction: column;

}
.canvasImg-exc{
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  height: 110rpx;
  line-height: 110rpx;
  color: #000;
  text-align: center;
  width: 100%;
}
button.huoqutupianquanxian{
  width: 300rpx;
  background: #fff;
  color: #333;
  font-size: 30rpx;
  padding-bottom: 10rpx;
}
button::after {
  border: none;
}

button {
  background-color: transparent;
  padding: 0;
  margin: 0;
  line-height:inherit;
}

button {
  border-radius:0;
}